msgstr ""

#: src/libslic3r/PrintConfig.cpp:399 src/libslic3r/PrintConfig.cpp:805
msgid "Hilbert Curve"
msgstr ""

#: src/libslic3r/PrintConfig.cpp:400 src/libslic3r/PrintConfig.cpp:806
